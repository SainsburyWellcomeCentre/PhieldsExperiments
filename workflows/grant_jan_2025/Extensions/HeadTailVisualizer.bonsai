                                        <drw:Thickness>2</drw:Thickness>
                                        <drw:LineType>Connected8</drw:LineType>
                                      </Combinator>
                                    </Expression>
                                    <Expression xsi:type="WorkflowOutput" />
                                  </Nodes>
                                  <Edges>
