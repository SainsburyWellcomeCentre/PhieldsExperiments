                                          <drw:Val2>255</drw:Val2>
                                          <drw:Val3>255</drw:Val3>
                                        </drw:ExternalColor>
                                        <drw:HoleColor>
                                          <drw:Val0>0</drw:Val0>
                                          <drw:Val1>0</drw:Val1>
                                          <drw:Val2>0</drw:Val2>
                                          <drw:Val3>0</drw:Val3>
