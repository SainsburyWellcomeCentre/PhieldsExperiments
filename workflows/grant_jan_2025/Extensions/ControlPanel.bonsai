      </Expression>
      <Expression xsi:type="GroupWorkflow">
        <Name>HeadTail</Name>
        <Workflow>
          <Nodes>
      <Expression xsi:type="ExternalizedMapping">
        <Property Name="Name" DisplayName="HeadTail" Description="HeadTail group observable" Category="Subject" />
      </Expression>
      <Expression xsi:type="SubscribeSubject">
        <Name>HeadTailObservable</Name>
      </Expression>
            <Expression xsi:type="SubscribeSubject">
              <Name>NumberOfTrackingSubjects</Name>
            </Expression>
            <Expression xsi:type="PropertyMapping">
              <PropertyMappings>
                <Property Name="Count" />
              </PropertyMappings>
      </Expression>
      <Expression xsi:type="Combinator">
        <Combinator xsi:type="rx:Take">
          <rx:Count>2</rx:Count>
        </Combinator>
      </Expression>
      <Expression xsi:type="rx:SelectMany">
        <Workflow>
          <Nodes>
            <Expression xsi:type="WorkflowInput">
              <Name>Source1</Name>
            </Expression>
            <Expression xsi:type="Combinator">
              <Combinator xsi:type="rx:Take">
                <rx:Count>1</rx:Count>
              </Combinator>
            </Expression>
            <Expression xsi:type="rx:AsyncSubject">
              <Name>Source</Name>
            </Expression>
            <Expression xsi:type="SubscribeSubject">
              <Name>Source</Name>
            </Expression>
            <Expression xsi:type="Combinator">
              <Combinator xsi:type="rx:Merge" />
            </Expression>
            <Expression xsi:type="SubscribeSubject">
              <Name>Source</Name>
            </Expression>
            <Expression xsi:type="MemberSelector">
              <Selector>Key</Selector>
            </Expression>
            <Expression xsi:type="Combinator">
              <Combinator xsi:type="rx:WithLatestFrom" />
            </Expression>
            <Expression xsi:type="WorkflowOutput" />
          </Nodes>
          <Edges>
            <Edge From="0" To="1" Label="Source1" />
            <Edge From="1" To="2" Label="Source1" />
            <Edge From="3" To="4" Label="Source1" />
            <Edge From="4" To="7" Label="Source1" />
            <Edge From="5" To="6" Label="Source1" />
            <Edge From="6" To="7" Label="Source2" />
            <Edge From="7" To="8" Label="Source1" />
          </Edges>
        </Workflow>
      </Expression>
      <Expression xsi:type="Disable">
        <Builder xsi:type="rx:ToDictionary">
          <rx:KeySelector>Item2</rx:KeySelector>
          <rx:ElementSelector>Item1.Value.Item1.Heading</rx:ElementSelector>
        </Builder>
      </Expression>
      <Expression xsi:type="viz:RollingGraphBuilder">
        <viz:IndexSelector>Item2</viz:IndexSelector>
        <viz:ValueSelector>Item1.Value.Item1.Heading</viz:ValueSelector>
        <viz:SymbolType>Default</viz:SymbolType>
        <viz:LineWidth>1</viz:LineWidth>
        <viz:Capacity xsi:nil="true" />
        <viz:Min xsi:nil="true" />
        <viz:Max xsi:nil="true" />
      </Expression>
      <Expression xsi:type="Disable">
        <Builder xsi:type="MemberSelector">
          <Selector>Item1.Item1.Velocity</Selector>
        </Builder>
      </Expression>
      <Expression xsi:type="Disable">
        <Builder xsi:type="viz:RollingGraphBuilder">
          <viz:SymbolType>None</viz:SymbolType>
          <viz:LineWidth>1</viz:LineWidth>
          <viz:Capacity xsi:nil="true" />
          <viz:Min xsi:nil="true" />
          <viz:Max xsi:nil="true" />
        </Builder>
      </Expression>
      <Expression xsi:type="Disable">
        <Builder xsi:type="VisualizerMapping" />
      </Expression>
      <Expression xsi:type="Disable">
        <Builder xsi:type="scr:ExpressionTransform">
          <scr:Expression>Math.Sqrt((it.X*it.X)+(it.Y*it.Y))</scr:Expression>
        </Builder>
      </Expression>
      <Expression xsi:type="Disable">
        <Builder xsi:type="VisualizerMapping" />
      </Expression>
      <Expression xsi:type="Disable">
        <Builder xsi:type="MemberSelector">
          <Selector>Item1.Item1.Heading</Selector>
        </Builder>
      </Expression>
      <Expression xsi:type="Disable">
        <Builder xsi:type="VisualizerMapping" />
      </Expression>
      <Expression xsi:type="Disable">
        <Builder xsi:type="gui:TableLayoutPanelBuilder">
          <gui:Enabled>true</gui:Enabled>
          <gui:Visible>true</gui:Visible>
          <gui:ColumnCount>1</gui:ColumnCount>
          <gui:RowCount>4</gui:RowCount>
          <gui:ColumnStyles />
          <gui:RowStyles />
          <gui:CellSpans />
        </Builder>
      </Expression>
      <Expression xsi:type="SubscribeSubject">
        <Name>ProcessedImage</Name>
      </Expression>
      <Expression xsi:type="ExternalizedMapping">
              <Property Name="Interval" DisplayName="VisualizerInterval" />
      </Expression>
      <Expression xsi:type="Combinator">
              <Combinator xsi:type="rx:SampleInterval">
                <rx:Interval>PT0.05S</rx:Interval>
        </Combinator>
      </Expression>
            <Expression xsi:type="SubscribeSubject">
              <Name>NumberOfTrackingSubjects</Name>
      </Expression>
            <Expression xsi:type="PropertyMapping">
              <PropertyMappings>
        <Property Name="NumberOfTrackingElements" />
              </PropertyMappings>
      </Expression>
      <Expression xsi:type="rx:SelectMany">
